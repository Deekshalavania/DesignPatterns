package com.epam.java.HomeTask1;

import java.util.List;

public class CalculateToalCost {
	
	public void totalCost(List list )
	{
		int total;
		System.out.println("*************Total weights**************");
		
		for(int i=0;i<list.size();i++) {

			total+=list.get(i).getsweetweight();
		}
		
		System.out.println("Total weight of all the sweet present is " +total);
	}

}
